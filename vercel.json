{
	"$schema": "https://openapi.vercel.sh/vercel.json",
	"bunVersion": "1.x",
	"buildCommand": "bun build:api",
	"installCommand": "bun install",
	"functions": {
		"server/dist/index.js": {
			"runtime": "@vercel/node@latest"
		}
	},
	"rewrites": [
		{
			"source": "/(.*)",
			"destination": "server/dist/index.js"
		}
	]
}
