{
	"$schema": "https://openapi.vercel.sh/vercel.json",
	"bunVersion": "1.x",
	"buildCommand": "bun build:api",
	"installCommand": "bun install",
	"builds": [
		{
			"src": "server/dist/index.js",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "server/dist/index.js"
		}
	]
}
